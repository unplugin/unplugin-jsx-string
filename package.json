{
  "name": "unplugin-jsx-string",
  "version": "0.6.0",
  "packageManager": "pnpm@8.6.5",
  "description": "Converts JSX to HTML strings at compile time.",
  "keywords": [
    "unplugin",
    "jsx",
    "tsx",
    "string",
    "html",
    "convert",
    "react"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "require": "./dist/api.js",
      "import": "./dist/api.mjs"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup && tsx scripts/postbuild.mts",
    "dev": "pnpm run -C playground dev",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "benchmark": "node benchmark/index.mjs",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@babel/parser": "^7.22.5",
    "@babel/types": "^7.22.5",
    "@rollup/pluginutils": "^5.0.2",
    "entities": "^4.5.0",
    "estree-walker": "^3.0.3",
    "jsesc": "^3.0.2",
    "magic-string": "^0.30.0",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^3.1.0",
    "@sxzz/prettier-config": "^1.0.3",
    "@types/benchmark": "^2.1.2",
    "@types/jsesc": "^3.0.1",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "benchmark": "^2.1.4",
    "bumpp": "^9.1.1",
    "eslint": "^8.44.0",
    "eslint-define-config": "^1.21.0",
    "fast-glob": "^3.3.0",
    "prettier": "^2.8.8",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@sxzz/prettier-config"
}
