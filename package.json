{
  "name": "unplugin-jsx-string",
  "version": "0.5.5",
  "packageManager": "pnpm@7.8.0",
  "description": "Converts JSX to HTML strings at compile time.",
  "keywords": [
    "unplugin",
    "jsx",
    "tsx",
    "string",
    "html",
    "convert",
    "react"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./cores": {
      "require": "./dist/cores.js",
      "import": "./dist/cores.mjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json,.md",
    "lint:fix": "pnpm run lint -- --fix",
    "build": "tsup && tsx scripts/postbuild.mts",
    "dev": "pnpm run -C playground dev",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "benchmark": "node benchmark/index.mjs",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@babel/parser": "^7.18.9",
    "@babel/types": "^7.18.9",
    "@rollup/pluginutils": "^4.2.1",
    "entities": "^4.3.1",
    "estree-walker": "^3.0.1",
    "jsesc": "^3.0.2",
    "magic-string": "^0.26.2",
    "unplugin": "^0.8.0"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^2.3.5",
    "@types/benchmark": "^2.1.1",
    "@types/jsesc": "^3.0.1",
    "@types/node": "*",
    "@types/react": "^18.0.15",
    "benchmark": "^2.1.4",
    "bumpp": "^8.2.1",
    "eslint": "^8.21.0",
    "eslint-define-config": "^1.6.0",
    "fast-glob": "^3.2.11",
    "prettier": "^2.7.1",
    "tsup": "^6.2.1",
    "tsx": "^3.8.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.20.2"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
